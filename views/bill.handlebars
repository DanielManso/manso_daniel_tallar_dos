<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Corkcicle</title>
     <link href="/style/NewStyle/mainStore.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">
</head>

<body>
    
    {{>nav}}

<section class="infoProduct">

     <article class="infoProduct__image">

         <img src="{{ product__img}}" alt="productImage">

    </article>

    <article class="description">

         <div class="description__text">
             <h1>{{product__name}} {{ product__collectio}} </h1>
               <h2>{{product__price}}</h2>
             <h3>{{product__size}}</h3>
             <p class="text__1">
               {{product__description}}
             </p>

             <lu class="text__2">
                 <li>
                     Antideslizante, fondo de silicona.

                 </li>
                     

                 <li>
                      Laterales de fácil agarre.

                 </li>

                 <li>
                     Tapón de rosca.

                 </li>
            

             </lu>
         </div>

         <div class="description__options">
          
                <a href="" class="description__buy">
                    <button>AÑADIR AL CARRITO</button>
                </a>
                
            
         </div>      
     </article>         
                 
    </section>  
</section>
        
       <section class="tags"> 

        <article class="tags__tagOne">

            <img src="./imagenesTienda/iconoCaract1.png" alt="">

        </article>

        <article class="tags__tagTwo">

            <img src="./imagenesTienda/iconoCaract2.png" alt="">

        </article>

        <article class="tags__tagThree">

            <img src="./imagenesTienda/iconoCaract3.png" alt="">

        </article>

    </section>

     </section>

       <section class="return">

      <article class="return__options">
        <a href="" class="return__clik">
          <h3>VOLVER ARRIBA</h3>
          <img src="./imagenesTienda/iconoFlecha.png" alt="return">
        </a>


      </article>

    </section>
    
    {{>landing}}
 
    {{>footer}}

    </main>

    <script src="/tienda.js"></script>
</body>

</html>