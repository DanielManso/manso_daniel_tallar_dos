<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Corkcicle</title>
    <link href="./style/main.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">
</head>

<body>
    <section id="headerimg">
     {{>nav}}
     {{>header}}
    </section>

    <section class="filter">

        <article class="filter__colour">

            <button class="filter__one">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__two">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__three">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__four">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__five">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__sixe">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__seven">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__eight">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>

            <button class="filter__ten">
             <img src="./imagenesTienda/filtroTres1.png" >
            </button>
        </article>

        <article class="filter__size">

            <button class="filter__everyone">
             <img src="./imagenesTienda/filtroTres1.png" >
             <h3>Todos</h3>
            </button>

            <button class="filter__25OZ">
             <img src="./imagenesTienda/filtroTres2.png" >
             <h3>25 OZ</h3>
            </button>

            <button class="filter__16OZ">
             <img src="./imagenesTienda/filtroTres3.png" >
             <h3>16 OZ</h3>
            </button>

            <button class="filter__9OZ">
             <img src="./imagenesTienda/filtroTres4.png" >
             <h3>9 OZ</h3>
            </button>
        </article>
       
    </section>
</section>

<main class="prducts">

    <section class="prduct">

             <article class="product__parent">

                <img id="product_one" class="product__image" src="{{imageProduct}}" > 
                <h2 class="product__name"> {{nameProduct}} </h2>
                <h3 class="product__size">{{sizeProduct}}</h3>
                <button>{{priceProduct}}</button>    

            </article>                                
    </section>

    <section class="return">

        <article class="return__options">
         <h3>VOLVER ARRIBA</h3>
         <img src="./imagenesTienda/iconoFlecha.png" alt="return">

        </article>
        
    </section>
                 
     {{>landing}}

     {{>footer}}
    
    </main>

    <script src="./style/script.js"></script>
</body>

</html>